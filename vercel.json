{
  "version": 2,
  "routes": [
    {
      "src": "/(.*\\.css)",
      "dest": "/$1",
      "headers": {
        "Content-Type": "text/css"
      }
    },
    {
      "src": "/(.*\\.js)",
      "dest": "/$1",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/$1",
      "headers": {
        "Content-Type": "text/html"
      }
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
